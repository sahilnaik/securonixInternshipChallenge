<head>
    <script src="https://cdn.plot.ly/plotly-2.6.3.min.js"></script>
    <title>Dashboard</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="../../public/dashboard.css" />
  </head>
  <body>
<main>
  

  
    <h1>Threat Hunting</h1>
    <div class="container">
      <div class="row">
        <div id="myDiv" class="col"></div>
        <div class="col">
          <div class="spacing"></div>
          <div class="row">
            <div id="meter" class="col"></div>
          </div>
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="row">
          <div class="col">
            <div class="details">
              {{user}} Threat Hunting
              <br>
              <br>
              <button class="btn btn-primary" id="editbtn" onclick="window.location.href='/edit'">Edit</button>
              <button class="btn btn-secondary" id="logoutbtn" onclick="window.location.href='/logout'">Log Out</button>
            </div>
            
          </div>
          <div class="col info">
              <div>
                <div class="foo blue"></div>

                <div>
                  Define
                </div>
              </div>
              <br>
              <div>
                <div class="foo red"></div>

                <div>
                  Manage
                </div>
              </div>
              <br>
              <div>
                <div class="foo green"></div>

                <div>
                  Use
                </div>
              </div>
          </div>

        </div>
        <div class="row cont">
          <div>Establishing threat hunting goals</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar define"
                 
                role="progressbar"
                aria-valuenow={{est}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{est}}%;"
              >
                {{est}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Current coverage of threat hunting goals</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{cur}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{cur}}%;"
              >
                {{cur}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Hirining personnel dedicated to threat hunting</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{hir}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{hir}}%;"
              >
                {{hir}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Formulating a threat hunting hypothesis</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{fr}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{fr}}%;"
              >
                {{fr}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Acquiring specialized datasets and tools</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{acq}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{acq}}%;"
              >
                {{acq}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Threat hunting training.</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{thr}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{thr}}%;"
              >
                {{thr}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>SOC members who can develop needed cybersecurity scripts</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{soc}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{soc}}%;"
              >
                {{soc}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Ability to scale threat hunting program.</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar manage"
                 
                role="progressbar"
                aria-valuenow={{scale}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{scale}}%;"
              >
                {{scale}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Utlzing full packet capture</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar use"
                 
                role="progressbar"
                aria-valuenow={{utFull}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{utFull}}%;"
              >
                {{utFull}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Utlzing windows registry keys</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar use"
                 
                role="progressbar"
                aria-valuenow={{utWin}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{utWin}}%;"
              >
                {{utWin}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />
          <div>Utlzing system memory</div>
          <div class="col-sm-10">
            <div class="progress">

              <div
                class="progress-bar use"
                 
                role="progressbar"
                aria-valuenow={{utSys}}
                aria-valuemin="0"
                aria-valuemax="100"
                style="width: {{utSys}}%;"
              >
                {{utSys}}
              </div>
            </div>
            <div class="start">0</div>
            <div class="end">100</div>
          </div>
          <br />

        </div>
      </div>
    </div>
    <br>

    <script>
      var data = [ { domain: { x: [0, 1], y: [0, 1] }, value:
      {{overall}}, title: { text: "Threat Hunting" }, type: "indicator", mode:
      "gauge+number" } ]; 
      var layout = { width: 600, height: 500, margin: { t:
      0, b: 0 } }; Plotly.newPlot('myDiv', data, layout); var data = [ { type:
      "indicator", mode: "number+gauge+delta", value: {{overallManage}}, delta: { reference: {{overallManage}}
      }, domain: { x: [0.25, 1], y: [0.08, 0.25] }, title: { text: "Manage" },
      gauge: { shape: "bullet", axis: { range: [null, 4] }, steps: [ { range:
      [0, 4], color: "gray" }, { range: [0, 4], color: "lightgray" } ], bar: {
      color: "black" } } }, { type: "indicator", mode: "number+gauge+delta",
      value: {{overallUse}}, delta: { reference: {{overallUse}} }, domain: { x: [0.25, 1], y: [0.4, 0.6]
      }, title: { text: "Use" }, gauge: { shape: "bullet", axis: { range: [null,
      4] }, steps: [ { range: [0, 4], color: "lightgray" } ], bar: { color:
      "black" } } }, { type: "indicator", mode: "number+gauge+delta", value: {{overallDefine}},
      delta: { reference: {{overallDefine}} }, domain: { x: [0.25, 1], y: [0.7, 0.9] }, title: {
      text: "Define" }, gauge: { shape: "bullet", axis: { range: [null, 4]
      }, steps: [ { range: [0, 4], color: "lightgray" } ], bar: { color: "black"
      } } } ]; var layout = { width: 600, height: 250, margin: { t: 10, r: 25,
      l: 25, b: 10 } }; Plotly.newPlot('meter', data, layout);


    </script>
 

</main>
 </body>
